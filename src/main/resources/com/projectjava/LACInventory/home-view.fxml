<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #FFFFFF;"
            xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.projectjava.LACInventory.Controller">
   <cursor>
      <Cursor fx:constant="DEFAULT"/>
   </cursor>
   <Button layoutX="253.0" layoutY="246.0" mnemonicParsing="false" onAction="#changeScreenMenu" prefHeight="51.0"
           prefWidth="95.0" style="-fx-background-color: #F8BC1E;" text="START" textFill="WHITE">
      <font>
         <Font name="Arial Rounded MT Bold" size="18.0"/>
      </font>
   </Button>
   <Label contentDisplay="CENTER" layoutX="158.0" layoutY="118.0" prefHeight="76.0" prefWidth="284.0"
          text="Welcome to L.A.C inventory " textFill="#0093b9">
      <font>
         <Font name="System Bold" size="24.0"/>
      </font>
   </Label>
   <ImageView fitHeight="80.0" fitWidth="80.0" layoutX="517.0" layoutY="3.0" pickOnBounds="true" preserveRatio="true">
      <Image url="@logo.png"/>
   </ImageView>
</AnchorPane>
